{
  "name": "@verdigris/libssh2.js",
  "version": "0.1.7",
  "description": "A WebAssembly port of libssh2 that enables SSH client functionality in JavaScript environments",
  "main": "dist/libssh2.js",
  "type": "module",
  "scripts": {
    "prepare": "./download-vendors.sh",
    "prebuild": "pnpm build:vendors",
    "build": "./build.sh && tsc --declaration --outDir dist",
    "build:vendors": "./build-vendors.sh",
    "build:docker": "docker build --output=type=tar,dest=libssh2.tar . && tar -xf libssh2.tar -C dist && rm libssh2.tar",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "ssh",
    "ssh2",
    "libssh2",
    "wasm",
    "webassembly",
    "emscripten",
    "terminal",
    "pty",
    "websocket",
    "browser",
    "nodejs"
  ],
  "author": "Andrew Jo <andrew@verdigris.co>",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VerdigrisTech/libssh2.js.git"
  },
  "bugs": {
    "url": "https://github.com/VerdigrisTech/libssh2.js/issues"
  },
  "homepage": "https://github.com/VerdigrisTech/libssh2.js#readme",
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "types": "index.d.ts",
  "files": [
    "dist/libssh2.js",
    "dist/libssh2.wasm",
    "index.d.ts"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "exports": {
    ".": {
      "import": "./dist/libssh2.js",
      "require": "./dist/libssh2.js",
      "types": "./index.d.ts"
    }
  },
  "sideEffects": false,
  "packageManager": "pnpm@10.14.0+sha256.297534e65d5842450539c1e8022c8831ab5e1fe2eb74664787a815519542d620",
  "vendors": {
    "libssh2": {
      "url": "https://github.com/libssh2/libssh2/releases/download/libssh2-1.11.1/libssh2-1.11.1.tar.gz",
      "sha256": "d9ec76cbe34db98eec3539fe2c899d26b0c837cb3eb466a56b0f109cabf658f7"
    },
    "openssl": {
      "url": "https://github.com/openssl/openssl/releases/download/openssl-3.5.2/openssl-3.5.2.tar.gz",
      "sha256": "c53a47e5e441c930c3928cf7bf6fb00e5d129b630e0aa873b08258656e7345ec"
    },
    "zlib": {
      "url": "https://www.zlib.net/zlib-1.3.1.tar.gz",
      "sha256": "9a93b2b7dfdac77ceba5a558a580e74667dd6fede4585b91eefb60f03b72df23"
    }
  }
}
